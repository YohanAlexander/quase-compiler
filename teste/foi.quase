classe _Chave filha da classe _Cifra;


classe _Cifra começa
	função int Codificar(int t_mensagem) t_mensagem
termina

class _Chave começa
	cons int SEED := 0b101001;
	var real escala;


	procedimento SetEscala() começa
		se ((escala % 2e0 == 0E3) e true ou !false)
			escala = -4e-2;
		senão
			escala = 7E-5 + 1 - 2 * 3 / 4;
	termina

	função int Codificar(int t_mensagem) começa
		se (false) então
			t_mensagem + SEED
		senão
			t_mensagem
	termina
termina

class _TelefoneDeLata começa
	objeto _IO console;

	=> procedimento Main() começa
		var int x;
		cons real y := 3.4;

		se (true) x = console.read();
		enquanto (false) console.print(123);
	termina
termina